{
  "name": "ngx-synaps-pics-app",
  "version": "0.0.0-development",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod ngx-synaps-pics",
    "test": "ng test ngx-synaps-pics --no-watch",
    "lint": "ng lint ngx-synaps-pics",
    "package": "npm publish dist/ngx-synaps-pics",
    "publish": "npm publish ./dist/ngx-synaps-pics",
    "publish:beta": "npm publish --tag=next ./dist/ngx-synaps-pics",
    "semantic-release-pre": "semantic-release pre",
    "semantic-release-post": "semantic-release post",
    "e2e": "ng e2e",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release/npm",
        {
          "pkgRoot": "./dist/ngx-synaps-pics"
        }
      ]
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.2.13",
    "@angular/common": "^7.2.13",
    "@angular/compiler": "^7.2.13",
    "@angular/core": "^7.2.13",
    "@angular/forms": "^7.2.13",
    "@angular/http": "^7.2.13",
    "@angular/platform-browser": "^7.2.13",
    "@angular/platform-browser-dynamic": "^7.2.13",
    "@angular/router": "^7.2.13",
    "core-js": "^2.5.4",
    "rxjs": "^6.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "^7.2.13",
    "@angular/language-service": "^7.2.13",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "protractor": "~5.3.0",
    "semantic-release": "^15.13.3",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.25.5",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synapshq/ngx-synaps-pics.git"
  }
}
